{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-main",
  "projects": {
    "api-main": {
      "type": "application",
      "root": "apps/api-main",
      "entryFile": "main",
      "sourceRoot": "apps/api-main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-main/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "calatog-service": {
      "type": "application",
      "root": "apps/calatog-service",
      "entryFile": "main",
      "sourceRoot": "apps/calatog-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/calatog-service/tsconfig.app.json"
      }
    },
    "iam-service": {
      "type": "application",
      "root": "apps/iam-service",
      "entryFile": "main",
      "sourceRoot": "apps/iam-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/iam-service/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}